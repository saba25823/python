class Solution(object):
    def frequencySort(self, s):
        """
        :type s: str
        :rtype: str
        """
        d = {}
        for i in s:
            if( i in d):
                d[i] = d[i] +1
            else:
                d[i] = 1
        s_d = sorted(d.items(), key = lambda y : -y[1])
        resultString = ""
        for key, value in s_d:
            for i in range(value):
                resultString += key
        return resultString
        
