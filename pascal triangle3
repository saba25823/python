def generateRow(row):
    ans=1
    result=[ans]
    for col in range(1,row):
        ans=ans*(row-col)
        ans=ans//(col)
        result.append(ans)
    return result
finalResult=[]
row=int(input("enter no.of rows"))
for row in range(1,row+1):
    finalResult.append(generateRow(row))
for lst in finalResult:
    print(*lst)
         

